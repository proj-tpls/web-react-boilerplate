{
  "name": "web-react-boilerplate",
  "version": "0.0.1",
  "description": "a boilerplate of pc web composed with dva、react、antd, which supports node version 8.*.*.",
  "scripts": {
    "build:dev": "cross-env NODE_ENV=development webpack",
    "build:prd": "cross-env NODE_ENV=production webpack",
    "dll:dev": "cross-env NODE_ENV=development webpack --config webpack.dll.config.js",
    "dll:prd": "cross-env NODE_ENV=production webpack --config webpack.dll.config.js",
    "start": "cross-env NODE_ENV=development webpack-dev-server",
    "analyze": "cross-env NODE_ENV=development ANALYZE=1 webpack",
    "lint": "prettier src/** --write && eslint src/**/*.js",
    "compile": "torenia compile",
    "1st-build:dev": "npm run compile && npm run dll:dev && npm run build:dev",
    "1st-build:prd": "npm run compile && npm run dll:prd && npm run build:prd"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-export-default-from": "^7.10.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.10.4",
    "@babel/plugin-proposal-optional-chaining": "^7.11.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.10.5",
    "@babel/polyfill": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/runtime": "7.0.0-beta.46",
    "add-asset-html-webpack-plugin": "^3.1.3",
    "antd": "3.26.18",
    "axios": "^0.20.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-react-require": "^3.0.3",
    "copy-webpack-plugin": "^4.6.0",
    "cross-env": "6.0.3",
    "css-loader": "^1.0.1",
    "dva": "^2.4.1",
    "dva-loading": "^2.0.6",
    "echarts": "^4.8.0",
    "echarts-for-react": "^2.0.16",
    "eslint": "^5.9.0",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "lodash": "^4.17.20",
    "microdict": "^0.0.2",
    "mini-css-extract-plugin": "^0.4.4",
    "moment": "^2.22.2",
    "node-sass": "^4.14.1",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-mixins": "^6.2.0",
    "postcss-nested": "^4.1.0",
    "postcss-url": "^8.0.0",
    "prettier": "^1.15.2",
    "react": "^16.6.0",
    "react-authority": "^1.0.4",
    "react-dom": "^16.6.0",
    "react-helmet": "^5.2.0",
    "react-split-pane-fixed": "^2.0.0",
    "sass-loader": "^7.1.0",
    "socket.io-client": "2.3.0",
    "style-loader": "^0.23.1",
    "stylelint": "^9.8.0",
    "stylelint-config-standard": "^18.2.0",
    "tiny-pinyin": "^1.3.0",
    "torenia": "^1.0.0-alpha.14",
    "torenia-cli": "^1.0.0-alpha.7",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.23.1",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "torenia": {
    "src": "src"
  }
}
